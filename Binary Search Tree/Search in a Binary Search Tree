class BST {
    // Function to search a node in BST.
    boolean search(Node root, int x) {
        // Your code here
        Node curr=root;
        while(curr!=null){
            if(curr.data==x)return true;
            else if(curr.data<x)curr=curr.right;
            else curr=curr.left;
        }
        return false;
    }
}
